package br.com.bytebank.banco.teste;

public class Teste {
	public static void main(String[] args) {
		
		int idade1 = 29;
		int idade2 = 34;
		int idade3 = 23;
		int idade4 = 75;
		int idade5 = 53;
				
		int[] idades = new int[5];
		
		for(int i = 0; i < idades.length; i++) {
			
			idades[i] = i+1;
		}
		System.out.println(idades[]);
		
	}
}
